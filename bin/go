#!/usr/bin/env python

import sys
from os.path import join, dirname, exists

# Use the local go.py if we are in the source tree.
source_tree_go = join(dirname(__file__), "..", "lib", "go.py")
if exists(source_tree_go):
    sys.path.insert(0, dirname(source_tree_go))
    try:
        from go import main
    finally:
        del sys.path[0]
else:
    from go import main

if __name__ == "__main__":
    sys.exit( main(sys.argv) )
